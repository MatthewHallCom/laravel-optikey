{
    "name": "riipandi/laravel-optikey",
    "description": "Use UUID or Ulid as optional or primary key in Laravel.",
    "keywords": ["laravel", "uuid", "ulid"],
    "type": "library",
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "^5.8|^6.0|^7.0",
        "robinvdvleuten/ulid": "^4.0"
    },
    "license": "Apache-2.0",
    "authors": [{
        "name": "Aris Ripandi",
        "email": "aris@ripandi.id"
    }],
    "autoload": {
        "psr-4": {
            "Riipandi\\LaravelOptiKey\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Riipandi\\LaravelOptiKey\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">5.0"
    }
}
